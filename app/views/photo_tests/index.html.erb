<p id="notice"><%= notice %></p>

<h1>Listing Photo Tests</h1>

<table>
  <thead>
    <tr>
      <th>Photo</th>
      <th>Filename</th>
      <th>User</th>
      <th>Time</th>
      <th>Caption</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photo_tests.each do |photo_test| %>
      <tr>
        <td><%= photo_test.photo_id %></td>
        <td><%= photo_test.filename %></td>
        <td><%= photo_test.user_id %></td>
        <td><%= photo_test.time %></td>
        <td><%= photo_test.caption %></td>
        <td><%= link_to 'Show', photo_test %></td>
        <td><%= link_to 'Edit', edit_photo_test_path(photo_test) %></td>
        <td><%= link_to 'Destroy', photo_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo test', new_photo_test_path %>
